<!DOCTYPE html>

<html>
    <head></head>
    <body>
        <div class="container">
            <h1>Users</h1>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">User Id</th>
                        <th scope="col">Name</th>
                        <th scope="col">Email</th>
                        <th scope="col">Password</th>
                        <th scope="col">Phone Number</th>
                    </tr>
                </thead>
                <tbody>
                    <% @users.each do |u|%>
                        <tr>
                            <th scope="row"><%= u.id%> </th>
                            <td><%= u.name%></td>
                            <td><%= u.email%></td>
                            <td><%= u.password%></td>
                            <td><%= u.phone_number%></td>
                        </tr>
                    <%end%>
                </tbody>
            </table>
        </div>
    </body>
</html>